{
    "name": "dmishh/recaptcha-bundle",
    "description": "reCAPTCHA support for Symfony2 with easy-to-use integration to Security Component.",
    "keywords": ["symfony2", "bundle", "recaptcha"],
    "homepage": "https://github.com/dmishh/RecaptchaBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Scherbina",
            "homepage": "http://dmishh.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.1",
        "dmishh/recaptcher": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}